      <ul>
      <% @account.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <h2>This is account is a…</h2><br />
    <%= f.radio_button :is_business, 'false' %> Person<br />
    <%= f.radio_button :is_business, 'true' %> Business<br />
  </div>

  <div class="field">
    <%= f.label :company_name %><br>
    <%= f.text_field :company_name %>
  </div>

<hr />
<h2>Contact Info</h2><br />
  <div class="field">
    <%= f.label :first_name %><br>
    <%= f.text_field :first_name %>
  </div>
  <div class="field">
    <%= f.label :last_name %><br>
<!-- START_HIGHLIGHT -->
    <%= f.text_field :last_name %>
    
<!-- END_HIGHLIGHT -->
  </div>
  <div class="field">
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </div>
  
  <div>
      <%= f.label :primary_phone_number %><br />
      <%= f.text_field :primary_phone_number %>
  </div>

  <div>
      <%= f.label :secondary_phone_number %><br />
      <%= f.text_field :secondary_phone_number %>
  </div>

  <div>
      <%= f.label :mobile_phone %><br />
      <%= f.text_field :mobile_phone %>
  </div>
  
  <div>
      <%= f.label :fax %><br />
      <%= f.text_field :fax %>
  </div>
  
  <h2>Additional Info </h2>
  
  
  <div>
      <%= f.label :referral_source %><br />
      <%= f.text_field :referral_source %>
  </div>
  
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>


